{
    "name":        "hostnet/asset-bundle",
    "type":        "symfony-bundle",
    "description": "Bundle to glue Assets",
    "license":     "MIT",
    "require": {
        "php":               ">=7.1",
        "hostnet/asset-lib": "dev-master",
        "symfony/symfony":   "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "hostnet/phpcs-tool": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Hostnet\\Bundle\\AssetBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hostnet\\Bundle\\AssetBundle\\": "test/"
        }
    },
    "repositories": {
        "hostnet": {
            "type": "composer",
            "url":  "https://packagist.hostnetbv.nl"
        }
    },
    "archive": {
        "exclude": [
            "/test"
        ]
    }
}
