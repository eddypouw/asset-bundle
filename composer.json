{
    "name":        "hostnet/asset-bundle",
    "type":        "symfony-bundle",
    "description": "Hostnet asset bundle allows for configuring the hostnet/asset-lib from within your symfony app",
    "license":     "MIT",
    "authors": [
        {
            "name":  "Yannick de Lange",
            "email": "ydelange@hostnet.nl"
        }
    ],
    "require": {
        "php":                      "^7.1",
        "hostnet/asset-lib":        "dev-master",
        "symfony/framework-bundle": "^3.3",
        "symfony/http-foundation":  "^2.7|^3.0",
        "symfony/process":          "^3.3"
    },
    "require-dev": {
        "hostnet/phpcs-tool": "^5.2",
        "phpunit/phpunit":    "^6.3",
        "twig/twig":          "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Hostnet\\Bundle\\AssetBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hostnet\\Bundle\\AssetBundle\\": "test/"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable"     : true,
    "archive": {
        "exclude": [
            "/test"
        ]
    }
}
